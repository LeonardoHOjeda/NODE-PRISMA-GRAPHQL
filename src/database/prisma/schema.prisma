// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DB_URL")
}

model seed {
  id        String   @id
  seeded_at DateTime @default(now())

  @@map("prisma_seeders")
}
